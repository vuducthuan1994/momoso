<!-- Wrapper Start -->
<div class="wrapper">
    <!-- Preloader Start -->
    <div class="preloader">
        <div class="loading-center">
            <div class="loading-center-absolute">
                <div class="object object_one"></div>
                <div class="object object_two"></div>
                <div class="object object_three"></div>
            </div>
        </div>
    </div>
    <!-- Preloader End -->

    <!-- Newsletter Popup Start -->
    {{>client/modal/subscribe}}
    <!-- Newsletter Popup End -->

    <!-- Header Area Start -->
    <header>
        <div class="container-fluid header-top-area header-sticky">
            <div class="row">
                <!-- Logo Start -->
                <div class="col-lg-2 col-md-2 col-sm-6 col-xs-5 full-col pl-0">
                    <div class="logo">
                        <a href="/"><img src="img/logo/logo.png" alt="brand-image"></a>
                    </div>
                </div>
                <!-- Logo End -->
                <div class="col-xs-12 visible-xs visible-control">
                    <ul class="search-form mobile-form">
                        <li>
                            <form action="#">
                                <input type="text" class="search" name="search" placeholder="Search for products...">
                            </form>
                            <i class="pe-7s-search"></i>
                        </li>
                    </ul>
                </div>
                <!-- Primary-Menu Start -->
                <div class="col-lg-7 col-md-7 col-sm-12  hidden-sm hidden-xs">
                    <div class="primary-menu">
                        <nav>
                            <ul class="primary-menu-list text-center">
                                <li><a href="/">Trang chủ</a></li>
                                <li><a href="/about-us">About Us</a></li>
                                <li><a href="/blog">Blogs</a></li>
                                <li><a href="/about-us">Liên hệ</a></li>
                            </ul>
                        </nav>
                    </div>
                </div>
                <!-- Primary-Menu End -->
                <!-- Header All Shopping Selection Start -->
                {{>client/header/header-all}}
                <!-- Header All Shopping Selection End -->
                <!-- Mobile Menu  Start -->
                <div class="mobile-menu visible-sm visible-xs">
                    <nav>
                        <ul>
                                      <li><a href="/">Trang chủ</a></li>
                                <li><a href="/about-us">About Us</a></li>
                                <li><a href="/blog">Blogs</a></li>
                                <li><a href="/about-us">Liên hệ</a></li>
                        </ul>
                    </nav>
                </div>
                <!-- Mobile Menu  End -->
            </div>
        </div>
    </header>
    <!-- Header Area End -->

    <!-- Slider Area Start -->
    {{>client/slider-home}}
    <!-- Slider Area End -->

    <!-- New Products Selection Start -->
    <div class="new-products-selection pb-80">
        <div class="container">
            <div class="row">
                <!-- Section Title Start -->
                <div class="col-xs-12">
                    <div class="section-title text-center mb-40">
                        <span class="section-desc mb-15">Top new in this week</span>
                        <h3 class="section-info">new products</h3>
                    </div>
                </div>
                <!-- Section Title End -->
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <!-- New Products Activation Start -->
                    <div class="new-products owl-carousel">
                        {{#each newProducts}}
                        <!-- Double Product Start -->
                        <div class="double-products">
                            <!-- Single Product Start -->
                            <div class="single-product mb-25">
                                <!-- Product Image Start -->
                                <div class="pro-img">
                                    <a href="{{ createURLProduct this.one.urlSeo}}">
                                        <img class="primary-img" src="{{getAvatarImage 'primary' this.one.listImages}}"
                                            alt="single-product">
                                        <img class="secondary-img"
                                            src="{{getAvatarImage 'secondary' this.one.listImages}}"
                                            alt="single-product">
                                    </a>
                                    <div class="quick-view">
                                        <a class="show-quick-modal" data-idproduct="{{this.one._id}}"><i
                                                class="pe-7s-look"></i>xem nhanh</a>
                                    </div>
                                    <span class="sticker-new">{{this.one.type}}</span>
                                </div>
                                <!-- Product Image End -->
                                <!-- Product Content Start -->
                                <div class="pro-content text-center">
                                    <h4><a href="{{ createURLProduct this.one.urlSeo}}">{{this.one.name}}</a></h4>
                                    <p data-price="{{this.one.price}}" class="product-price-vnd price">
                                        <span>{{this.one.price}}</span></p>
                                    <div class="action-links2">
                                        <a class="add-product-to-cart" data-product="{{objectToString this.one}}"
                                            data-toggle="tooltip" title="Add to Cart">add to cart</a>
                                    </div>
                                </div>
                                <!-- Product Content End -->
                            </div>
                            <!-- Single Product End -->
                            <!-- Single Product Start -->
                            <div class="single-product">
                                <!-- Product Image Start -->
                                <div class="pro-img">
                                    <a href="{{ createURLProduct this.two.urlSeo}}">
                                        <img class="primary-img" src="{{getAvatarImage 'primary' this.two.listImages}}"
                                            alt="single-product">
                                        <img class="secondary-img"
                                            src="{{getAvatarImage 'secondary' this.two.listImages}}"
                                            alt="single-product">
                                    </a>
                                    <div class="quick-view">
                                        <a class="show-quick-modal" data-idproduct="{{this.two._id}}" href="#"
                                            data-toggle="modal" data-target="#myModal"><i class="pe-7s-look"></i>xem nhanh</a>
                                    </div>
                                    <span class="sticker-new">{{this.two.type}}</span>
                                </div>
                                <!-- Product Image End -->
                                <!-- Product Content Start -->
                                <div class="pro-content text-center">
                                    <h4><a href="{{ createURLProduct this.two.urlSeo}}">{{this.two.name}}</a></h4>
                                    <p data-price="{{this.two.price}}" class="price product-price-vnd">
                                        <span>{{this.two.price}}</span></p>
                                    <div class="action-links2">
                                        <a class="add-product-to-cart" data-product="{{objectToString this.two}}"
                                            data-toggle="tooltip" title="Add to Cart">add to
                                            cart</a>
                                    </div>
                                </div>
                                <!-- Product Content End -->
                            </div>
                            <!-- Single Product End -->
                        </div>
                        <!-- Double Product End -->
                        {{/each}}
                    </div>
                    <!-- New Products Activation End -->
                </div>
            </div>
            <!-- Row End -->
        </div>
        <!-- Container End -->
    </div>
    <!-- New Products Selection End -->
    <!-- New Products Banner Start -->
    <div class="new-products-banner pb-100">
        <div class="container">
            <div class="row">
                <!-- Single Banner Start -->
                {{#each categorys}}
                <a href="{{createURLCategory this.urlSeo}}" class=" {{checkTypeCategory1 this.typeImage}}">
                    <div class="{{checkTypeCategory2 @index}}">
                        <img src="{{this.imageUrl}}" alt="product-banner">
                        <div class="banner-desc">
                            <div href="categorie-page.html">{{this.name}}</div>
                            <p>{{this.totalProduct}} sản phẩm</p>
                        </div>
                    </div>
                </a>
                {{/each}}
                <!-- Single Banner End -->
            </div>
            <!-- Row End -->
        </div>
        <!-- Container End -->
    </div>
    <!-- New Products Banner End -->
    <!-- Best Seller Products Start -->
    <div class="best-seller-products pb-100">
        <div class="container">
            <div class="row">
                <!-- Section Title Start -->
                <div class="col-xs-12">
                    <div class="section-title text-center mb-40">
                        <span class="section-desc mb-20">Top sale in this week</span>
                        <h3 class="section-info">best seller</h3>
                    </div>
                </div>
                <!-- Section Title End -->
            </div>
            <!-- Row End -->
            <div class="row">
                <div class="col-sm-12">
                    <!-- Best Seller Product Activation Start -->
                    <div class="best-seller new-products owl-carousel">
                        {{#each bestSellerProduct}}
                        <!-- Single Product Start -->
                        <div class="single-product">
                            <!-- Product Image Start -->
                            <div class="pro-img">
                                <a href="{{createURLProduct this.urlSeo}}">
                                    <img class="primary-img" src="{{getAvatarImage 'primary' this.listImages}}"
                                        alt="single-product">
                                    <img class="secondary-img" src="{{getAvatarImage 'secondary' this.listImages}}"
                                        alt="single-product">
                                </a>
                                <div class="quick-view">
                                    <a class="show-quick-modal" data-idproduct="{{this._id}}" data-toggle="modal"
                                        data-target="#myModal"><i class="pe-7s-look"></i>xem nhanh</a>
                                </div>
                                <span class="sticker-new">{{this.type}}</span>
                            </div>
                            <!-- Product Image End -->
                            <!-- Product Content Start -->
                            <div class="pro-content text-center">
                                <h4><a href="{{createURLProduct this.urlSeo}}">{{this.name}}</a></h4>
                                <p  data-price="{{this.price}}" class="price product-price-vnd">
                                    <span>{{this.price}}</span></p>
                                <div class="action-links2">
                                    <a data-toggle="tooltip" title="Add to Cart" href="cart.html">add to cart</a>
                                </div>
                            </div>
                            <!-- Product Content End -->
                        </div>
                        <!-- Single Product End -->
                        {{/each}}
                    </div>
                    <!-- Best Seller Product Activation Start -->
                    <div class="text-center shop-link-page mt-50"><a href="best-seller.html">Shop All Best
                            Sellers</a></div>
                </div>
            </div>
            <!-- Row End -->
        </div>
        <!-- Container End -->
    </div>
    <!-- Best Seller Products End -->
    <!-- Big Banner CSS Start -->
    <div class="big-banner-ads">
        <div class="big-banner">
            <div class="big-banner-desc text-center">
                <h2>GET OUR CATALOGUE</h2>
                <p>Receive free monthly catalogue showcasing the best in modern <br> furniture design with some
                    outstanding features!</p>
                <a href="categorie-page.html">shop now</a>
            </div>
        </div>
    </div>
    <!-- Big Banner CSS End -->
    <!-- Latest Blog Start -->
    <div class="latest-blog off-white-bg ptb-100">
        <div class="container">
            <div class="row">
                <!-- Section Title Start -->
                <div class="col-xs-12">
                    <div class="section-title text-center mb-40">
                        <span class="section-desc mb-20">Latest News From Our Blog</span>
                        <h3 class="section-info">LATEST BLOGS</h3>
                    </div>
                </div>
                <!-- Section Title End -->
            </div>
            <!-- Row End -->
            <div class="row">
                <div class="col-sm-12">
                    <!-- Blog Activation Start -->
                    <div class="blog owl-carousel">
                        {{#each newPosts}}
                        <!-- Single Blog Start -->
                        <div class="single-blog">
                            <div class="blog-img">
                                <a href="blog-details.html"><img src="{{this.thumb_image}}" alt="blog-image"></a>
                            </div>
                            <div class="blog-content">
                                <div class="blog-content-upper">
                                    <h6 class="blog-title"><a href="blog-details.html">{{this.title}}</a></h6>
                                    <p>{{getDescriptionArtiles this.short_desc}}</p>
                                </div>
                                <div class="blog-content-lower">
                                    <a >MOMO</a>
                                    <span class="f-right">{{getDateArtiles this.created_date}}</span>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                        <!-- Single Blog End -->
                    </div>
                    <!-- Blog Activation End -->
                </div>
            </div>
            <!-- Row End -->
        </div>
        <!-- Container End -->
    </div>
    <!-- Latest Blog End -->

    <!-- Footer Start -->
    <footer>
        <!-- Footer Top Start -->
        <div class="footer-top ptb-50">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="banner-slider owl-carousel">
                            <!-- Single Banner Start -->
                            <div class="single-banner">
                                <a href="#"><img src="img/banner-slider/1.png" alt="banner-image"></a>
                            </div>
                            <!-- Single Banner End -->
                            <!-- Single Banner Start -->
                            <div class="single-banner">
                                <a href="#"><img src="img/banner-slider/2.png" alt="banner-image"></a>
                            </div>
                            <!-- Single Banner End -->
                            <!-- Single Banner Start -->
                            <div class="single-banner">
                                <a href="#"><img src="img/banner-slider/3.png" alt="banner-image"></a>
                            </div>
                            <!-- Single Banner End -->
                            <!-- Single Banner Start -->
                            <div class="single-banner">
                                <a href="#"><img src="img/banner-slider/4.png" alt="banner-image"></a>
                            </div>
                            <!-- Single Banner End -->
                            <!-- Single Banner Start -->
                            <div class="single-banner">
                                <a href="#"><img src="img/banner-slider/5.png" alt="banner-image"></a>
                            </div>
                            <!-- Single Banner End -->
                            <!-- Single Banner Start -->
                            <div class="single-banner">
                                <a href="#"><img src="img/banner-slider/6.png" alt="banner-image"></a>
                            </div>
                            <!-- Single Banner End -->
                            <!-- Single Banner Start -->
                            <div class="single-banner">
                                <a href="#"><img src="img/banner-slider/3.png" alt="banner-image"></a>
                            </div>
                            <!-- Single Banner End -->
                        </div>
                    </div>
                </div>
                <!-- Row End -->
            </div>
            <!-- Container End -->
        </div>
        <!-- Footer Top End -->

        <!-- Footer Middle Start -->
        {{>client/footer/footer-middle}}
        <!-- Footer Middle End -->

        <!-- Footer Bottom Start -->
        {{>client/footer/footer-bottom}}
        <!-- Footer Bottom End -->
    </footer>
    <!-- Footer End -->
    <!-- Quick View Content Start -->
    {{>client/modal/quick-view-product}}
    <!-- Quick View Content End -->
</div>
<!-- Wrapper End -->