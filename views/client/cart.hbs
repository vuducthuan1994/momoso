                                            <a href="#"><img src="img/new-products/2_2.jpg" alt="cart-image" /></a>
                                        </td>
                                        <td class="product-name"><a href="#">{{this.name}}</a></td>
                                        <td class="product-price"><span class="amount">£165.00</span></td>
                                        <td class="product-quantity"><input type="number" value="{{this.quantity}}" /></td>
                                        <td class="product-subtotal product-price-vnd">{{{{this.price}}}}</td>
                                        <td class="product-remove"> <a href="#"><i data-productid="{{this._id}}" class="fa fa-times remove-product-from-cart" aria-hidden="true"></i></a></td>
