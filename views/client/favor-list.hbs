            <div class="container">
                <!-- Section Title Start -->
                <div class="section-title mb-50">
                    <h2>wish list</h2>
                </div>
                <!-- Section Title Start End -->
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <!-- Form Start -->
                        <form action="#">
                            <!-- Table Content Start -->
                            <div class="table-content table-responsive">
                                <table>
                                    <thead>
                                        <tr>
                                            <th class="product-remove">Remove</th>
                                            <th class="product-thumbnail">Image</th>
                                            <th class="product-name">Product</th>
                                            <th class="product-price">Unit Price</th>
                                            <th class="product-quantity">Stock Status</th>
                                            <th class="product-subtotal">add to cart</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each cart.listFavorProducts}}
                                        <tr id="{{this._id}}">
                                            <td class="product-remove"> <a href="#"><i data-productid="{{this._id}}"  class="remove-wish-list fa fa-times"
                                                        aria-hidden="true"></i></a></td>
                                            <td class="product-thumbnail">
                                                <a href="#"><img src="{{getActiveProductImage this.listImages}}"
                                                        alt="cart-image" /></a>
                                            </td>
                                            <td class="product-name"><a href="#">{{this.name}}</a></td>
                                            <td class="product-price"><span  data-price = "{{this.price}}"class="amount product-price-vnd">{{this.price}}</span></td>
                                            <td class="product-stock-status"><span>in stock</span></td>
                                            <td class="product-add-to-cart"><a class="add-product-to-cart"
                                                    data-product="{{objectToString this}}">add to cart</a></td>
                                        </tr>
                                        {{else}}
                                        <tr>
                                            <td colspan="6" class="text-warning">
                                                <p class="text-muted">Bạn chưa thêm sản phẩm nào vào danh sách !</p>
                                            </td>
                                        </tr>
                                        {{/each}}

                                    </tbody>
                                </table>
                            </div>
                            <!-- Table Content Start -->
                        </form>
                        <!-- Form End -->
                    </div>
                </div>
                <!-- Row End -->
            </div>
        </div>
